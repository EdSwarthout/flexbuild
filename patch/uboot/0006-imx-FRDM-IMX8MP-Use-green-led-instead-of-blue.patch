 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/dts/imx8mp-frdm.dts b/arch/arm/dts/imx8mp-frdm.dts
index 95ca15dfbf6..2517b8aaf74 100644
--- a/arch/arm/dts/imx8mp-frdm.dts
+++ b/arch/arm/dts/imx8mp-frdm.dts
@@ -22,7 +22,7 @@
 		compatible = "gpio-leds";
 
 		led-0 {
-			gpios = <&pcal6416 15 GPIO_ACTIVE_HIGH>;
+			gpios = <&pcal6416 14 GPIO_ACTIVE_HIGH>;
 			default-state = "on";
 		};
 	};
